{
    "version": 2,
    "builds": [
        {
            "src": "services/user-service/app/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "services/table-service/app/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "services/menu-service/app/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "services/order-service/app/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "services/kitchen-service/app/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "services/payment-service/app/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "docs/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/users/(.*)",
            "dest": "services/user-service/app/main.py"
        },
        {
            "src": "/api/tables/(.*)",
            "dest": "services/table-service/app/main.py"
        },
        {
            "src": "/api/menu/(.*)",
            "dest": "services/menu-service/app/main.py"
        },
        {
            "src": "/api/orders/(.*)",
            "dest": "services/order-service/app/main.py"
        },
        {
            "src": "/api/kitchen/(.*)",
            "dest": "services/kitchen-service/app/main.py"
        },
        {
            "src": "/api/payments/(.*)",
            "dest": "services/payment-service/app/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/docs/$1"
        }
    ]
}
